<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <android.support.v7.widget.LinearLayoutCompat
        android:id="@+id/mLinearProfile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_50sdp"
        android:background="@drawable/bg_dialog_profile"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Chi nhánh"
            android:textColor="@color/black"
            android:textSize="@dimen/_18sdp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/mTvAdd1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:gravity="center"
            android:paddingBottom="@dimen/_10sdp"
            android:paddingTop="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="226/20B Trưng Nữ Vương, Q. Hải Châu"
            android:textStyle="bold|italic" />

        <TextView
            android:id="@+id/mTvAdd2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingBottom="@dimen/_10sdp"
            android:paddingTop="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="106 Ỷ Lan Nguyên Phi, Q. Hải Châu"
            android:textStyle="bold|italic" />
        <TextView
            android:id="@+id/mTvAdd3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingBottom="@dimen/_10sdp"
            android:paddingTop="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="71 Hà Tông Quyền, Q. Cẩm Lệ"
            android:textStyle="bold|italic" />
        <TextView
            android:id="@+id/mTvAdd4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingBottom="@dimen/_10sdp"
            android:paddingTop="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="80 Bùi Dương Lịch. Lô 01 Cổn Chợ Nại Hiên Đông"
            android:textStyle="bold|italic" />
        <TextView
            android:id="@+id/mTvAdd5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingBottom="@dimen/_10sdp"
            android:paddingTop="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="Kiot 07, Chợ Lê Trạch, Hòa Tiến"
            android:textStyle="bold|italic" />

    </android.support.v7.widget.LinearLayoutCompat>

    <ImageView
        android:id="@+id/mImgCancelDialog"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_25sdp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/mLinearProfile"
        android:layout_marginTop="@dimen/dialog_cancel"
        android:background="@drawable/ic_btn_cancel_chat" />
</RelativeLayout>
